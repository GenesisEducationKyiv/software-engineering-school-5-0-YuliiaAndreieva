{
  "id": null,
  "uid": "weather-cache-timeseries",
  "title": "Weather API Cache - Line Chart Dashboard",
  "tags": ["weather", "cache", "redis"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "time": {
    "from": "now-5m",
    "to": "now"
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Cache Hits Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(weather_cache_hits[1m])",
          "refId": "A"
        }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 12, "h": 6 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Cache Misses Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(weather_cache_misses_total[1m])",
          "refId": "B"
        }
      ],
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 6 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Cache Errors Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(weather_cache_errors_total[1m])",
          "refId": "C"
        }
      ],
      "gridPos": { "x": 0, "y": 12, "w": 12, "h": 6 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Cache Skipped Sets Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "rate(weather_cache_skipped_total[1m])",
          "refId": "D"
        }
      ],
      "gridPos": { "x": 0, "y": 18, "w": 12, "h": 6 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Cache Key Count",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "weather_cache_key_count",
          "refId": "E"
        }
      ],
      "gridPos": { "x": 0, "y": 24, "w": 12, "h": 6 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" }
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Cache Operation Duration (Seconds)",
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.99, rate(weather_cache_operation_duration_seconds_bucket[1m]))",
          "refId": "F"
        }
      ],
      "gridPos": { "x": 0, "y": 30, "w": 12, "h": 6 },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "palette-classic" },
          "unit": "seconds"
        }
      }
    }
  ],
  "templating": { "list": [] },
  "annotations": { "list": [] },
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 0,
  "links": [],
  "liveNow": false
}